                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-9" data-bind="foreach: Lawyers">
                    <div class="card all_lawyer_view_list p-4 mb-3">
                        <div class="stamp">
                            <div class="stamp-inner shadow">
                                <i class="fas fa-star" data-bind="style: { color: isOnline? 'green' : '' }"></i>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2 d-flex justify-content-center justify-content-md-start">
                                @*<div class="shadow card-img-top rounded-circle" data-bind="attr:{src:profileImg}" alt="Card image cap"></div>*@
                                <div class="shadow card-img-top rounded-circle" data-bind="style: { backgroundImage: 'url(\'' + profileImg + '\')' }" alt="Card image cap"></div>
                            </div>
                            <div class="col-md-5 lawyer-data">
                                @{
                                    var n = Model.IsLtr ? "nameEn" : "name";
                                    var s = Model.IsLtr ? "specializationEn" : "specialization";
                                    var desc = Model.IsLtr ? "descriptionEn" : "description";
                                    <div class="lawyer-data-content mb-4">
                                        <a href="#" data-bind="click: $parent.goToDetails.bind($data)"><h5><small style="color:#51d3c3">@Localizer["Lawyer"] </small><strong data-bind="text: @n"></strong></h5></a>
                                        @*<p class="mb-2" data-bind="text: @desc"></p>*@
                                        <ul class="rating p-0 mb-2 d-inline my-3">
                                            <li class="fas fa-star" data-bind="css: rate > 0? 'active' : ''"></li>
                                            <li class="fas fa-star" data-bind="css: rate > 1? 'active' : ''"></li>
                                            <li class="fas fa-star" data-bind="css: rate > 2? 'active' : ''"></li>
                                            <li class="fas fa-star" data-bind="css: rate > 3? 'active' : ''"></li>
                                            <li class="fas fa-star" data-bind="css: rate > 4? 'active' : ''"></li>
                                        </ul>
                                        <div class="number-of-reviews p-2 d-inline">
                                            <span>@Localizer["JoinedRecently"]</span>
                                        </div>
                                    </div>
                                    <ul class="lawyer_info mt-md-2 mt-4 ">
                                        <li><i class="mr-2 mb-2 fas fa-gavel mainColor"></i>@Localizer["Specializedin"]<span class="main_dark" data-bind="text: @s"></span></li>
                                        <li><i class="mr-2 mb-2 fas fa-map-marker-alt mainColor"></i>Sidy Bishr : 44 Zien Albedeen st. branched from Gamal Abdeln</li>
                                        <li><i class="mr-2 mb-2 fas fa-money-bill-alt mainColor"></i>@Localizer["Fees"] <strong data-bind="text: $parent.setCurrency(fees)"></strong>&nbsp;@Localizer["EGP"]</li>
                                        <li><i class="mr-2 mb-2 fas fa-phone mainColor"></i><strong>16676</strong> Cost of regular call</li>
                                    </ul>
                                }
                            </div>
                            <div class="col-md-5">
                                <div data-bind="attr: {'id': 'carouselExampleControls_' + id}" width="90%" class="carousel slide px-3" data-ride="carousel">
                                    <div class="carousel-inner" data-bind="foreach: appointments">
                                        <div class="carousel-item" data-bind="css: $index()=== 0? 'active' : ''">
                                            <div class="card-group book_days">
                                                <div class="card">
                                                    <div class="card-header">
                                                        <span data-bind="text: dateTxt != ''? dateTxt : 'NoDataFound'"></span>
                                                    </div>
                                                    <div class="card-body">
                                                        <ul class="book_time" data-bind="foreach: times">
                                                            <li><a data-bind="text: timeFrom, click: function() {$root.book($parents[1],$data)}"></a></li>
                                                        </ul>
                                                    </div>
                                                    <div class="card-footer">
                                                        <span>@Localizer["BOOK"]</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <a class="carousel-control-prev" data-bind="attr: {'href': '#carouselExampleControls_' + id}" role="button" data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" data-bind="attr: {'href': '#carouselExampleControls_' + id}" role="button" data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
</div>

<script src="~/app/lawyer.js?id=1"></script>


